<div class="module spotlight" id="" data-metrics-module="">
    <div class="module-content">
        <div class="slider single">
            <div class="singleheader">
                <div class="slide">
                    <ul class="listHorizontal">
                        <% for (var i = 0; i < editorial.listHorizontal.length; i++) { %>
                        <li class="thumb video">
                            <a class="block img clearfix" data-metrics-link="1"
                               href="<%= editorial.listHorizontal[i].href %>">
                                <img src="<%= editorial.listHorizontal[i].src %>" alt=""/>
                            </a>
                            <a class="info clearfix" data-metrics-link="1"
                               href="<%= editorial.listHorizontal[i].href %>">
                                <h4><%= editorial.listHorizontal[i].h4 %></h4>
                                <div class="text"><%= editorial.listHorizontal[i].text %></div>
                            </a>
                        </li>
                        <% } %>
                    </ul>
                </div>
            </div>
            <div class="thumbs">
                <ul>
                    <% for (var i = 0; i < editorial.thumbs.length; i++) { %>
                    <li class="video">
                        <a href="<%= editorial.thumbs[i].href %>">
                            <img src="<%= editorial.thumbs[i].src %>" alt="butter melting in pan"/>
                            <div class="thumbtext">
                                <div class="text"><%= editorial.thumbs[i].text %></div>
                                <div class="action"><%= editorial.thumbs[i].action %></div>
                            </div>
                        </a>
                    </li>
                    <% } %>
                </ul>
            </div>
        </div>
    </div>
</div>
